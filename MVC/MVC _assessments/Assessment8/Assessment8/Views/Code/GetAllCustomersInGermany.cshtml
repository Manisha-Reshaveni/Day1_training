
@{
    ViewBag.Title = "GetAllCustomersInGermany";
}

<h2>GetAll Customers In Germany</h2>
<table>
    <thead>
        <tr>
            <th>CustomerID</th>
            <th>CompanyName</th>
            <th>ContactName</th>
            <th>Country</th>

        </tr>
    </thead>
    <tbody>
        @foreach (var customer in Model)
        {
<tr>
    <td>@customer.CustomerID</td>
    <td>@customer.CompanyName</td>
    <td>@customer.ContactName</td>
    <td>@customer.Country</td>
</tr>}
    </tbody>
</table>


